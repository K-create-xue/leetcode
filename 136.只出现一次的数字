for (int i = 0; i < sizeof(nums)/sizeof(nums[0]); ++i)
        {
            int count=0;
            for (int j = 0; j < sizeof(nums) / sizeof(nums[0]); ++j)
            {
                if (nums[i] == nums[j])
                    ++count;
            }
            if (count == 1)
            {
                cout << nums[i] << endl;
                return nums[i];
            }
        }
        
       时间复杂度太高
